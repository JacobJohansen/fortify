{
  "name": "fortify",
  "productName": "Fortify",
  "version": "1.0.23",
  "description": "",
  "main": "out/main.js",
  "scripts": {
    "start": "electron .",
    "build:main": "webpack --config scripts/webpack.main.config.js",
    "build:renderer": "webpack --config scripts/webpack.renderer.config.js",
    "build:prod": "npm run build:main & npm run build:renderer",
    "build:package": "electron-packager . --arch=x64 --out=build --icon=src/static/icons/tray/mac/icon.icns --overwrite=true --electron-version=1.7.6 --app-bundle-id=com.peculiarventures.fortify --no-prune",
    "build:dmg": "electron-installer-dmg build/Fortify-darwin-x64/Fortify.app fortify-mac-x64 --out=build --overwrite --icon=src/static/icons/tray/png/icon@2x.png",
    "build:pkg": "npm run build:prod && fortify-prepare && electron-packager /tmp/fortify-prod/x64 --arch=x64 --out=build --icon=src/static/icons/tray/mac/icon.icns --overwrite=true --electron-version=1.8.4 --app-bundle-id=com.peculiarventures.fortify --no-prune && npm run build:pkg:copy-nss",
    "build:pkg:copy-nss": "cp nss/* build/Fortify-darwin-x64/Fortify.app/Contents/MacOS",
    "build:msi_x64": "npm run build:prod && fortify-prepare && asar pack /tmp/fortify-prod/x64 ../fortify-prod/x64/resources/app.asar",
    "build:msi_x86": "npm run build:prod && fortify-prepare && asar pack /tmp/fortify-prod/x86 ../fortify-prod/x86/resources/app.asar",
    "build": "ts-node scripts/build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/asn1js": "0.0.1",
    "@types/classnames": "^2.2.10",
    "@types/extract-zip": "^1.6.2",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/request": "^2.48.3",
    "@types/rimraf": "^2.0.3",
    "@types/semver": "^6.2.0",
    "@types/websocket": "^0.0.40",
    "@types/xmldom": "^0.1.29",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.5.3",
    "electron": "8.2.4",
    "electron-packager": "^14.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "extract-zip": "^1.6.7",
    "fortify-prepare": "git+https://github.com/PeculiarVentures/fortify-prepare.git",
    "json-parser": "^3.1.2",
    "node-gyp": "^6.1.0",
    "node-sass": "^4.14.0",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.8.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "xmldom": "^0.1.27"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@peculiar/webcrypto": "^1.0.29",
    "@webcrypto-local/server": "^1.0.16",
    "asn1js": "^2.0.26",
    "classnames": "^2.2.6",
    "jose-jwe-jws": "github:microshine/js-jose",
    "lib-react-components": "0.3.84",
    "pkijs": "^2.1.88",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "request": "^2.88.0",
    "semver": "^6.3.0",
    "sudo-prompt": "^9.1.1",
    "winston": "^3.2.1"
  }
}
